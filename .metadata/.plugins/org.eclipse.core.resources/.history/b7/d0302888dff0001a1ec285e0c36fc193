package com.cts.bean;
import javax.persistence.*;

public class Loan {

	@Entity
	@Table(name = "User")
	public class User {
	    @Id
	    @GeneratedValue(strategy = GenerationType.AUTO)
	    private int id;
	    private String userName;
	    private String password;
	    private boolean active;
	    private String roles;


}
}
